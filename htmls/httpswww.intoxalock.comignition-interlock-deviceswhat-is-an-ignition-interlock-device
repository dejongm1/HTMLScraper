  rmnum = "b3d386483af42d9e";
  }
  
  else if(rmurl.search("dui-ignition-interlock-los-angeles-geo-fencing")>-1) {
  rmnum = "9ee86281905905bf";
  }
 
  var ssaUrl = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'centro.pixel.ad/iap/'+rmnum;
  new Image().src = ssaUrl;
  

</script>
