<script type="text/rocketscript">
    function Validate() {
        var zipCode = document.getElementById('dnn_dnnZipCode_txtZipCode1').value;
        var reg = /^\d+$/;
        if (zipCode.length != 5) {
            $.fancybox.close();
            invalidZip();
            $("#dnn_dnnZipCode_txtZipCode1").val('');
            return false;
        }
        else if (!reg.test(zipCode)) {
            $.fancybox.close();
            invalidZip();
            $("#dnn_dnnZipCode_txtZipCode1").val('');
            return false;
        }
        else {
            document.getElementById('dnn_dnnZipCode_hdnZipCode1').value = zipCode;
            return true;
        }
    }
    $(document).ready(function () {
        $("#zipCodePopupDiv input[type=text]").keyup(function (event) {
            if (event.which == 13) {
                if (Validate() == true) {
                    eval($("#dnn_dnnZipCode_lnkZipCode1").attr('href'));
                }
            }
        });
    });
</script>
<input type="hidden" name="dnn$dnnZipCode$hdnZipCode1" id="dnn_dnnZipCode_hdnZipCode1"/>
