<script type="text/rocketscript">

    $(document).ready(function () {
        var rx = /^US-(NM)$/;
        var map = $("#zip-code-map").vectorMap({
            map: "us_aea_en",
            regionStyle: {
                hover: {
                    fill: "#F8941E"
                },
                initial: {
                    fill: "#BCBCBC"
                }
            },
            zoomOnScroll: false,
            zoomMax: 1,
            zoomMin: 1,
            zoomButtons: false,
            backgroundColor: "transparent",
            onRegionClick: function (e, code) {
                mapClick(code);
            },
            onRegionLabelShow: function (event,label,code)
            {
                if(rx.test(code))
                    label.html(label.html() + '');
                else
                    label.html(label.html() + '');
            }
        });
        function mapClick(code) {
            var map = $("#zip-code-map").vectorMap('get', 'mapObject');
            location.href = $("a[data-statename=\"" + map.getRegionName(code).toLowerCase() + "\"]").attr("href");
        }

        $("#zipcodeContainer a").on("click", function(){
            if(/\d{5}/.test($("#zipcodeContainer input").val()))
                return true;
            $("#invalidZip span").html("Please enter a valid zip code<br/>&nbsp;");
            invalidZip();
            return false;
        });
    });
</script>
