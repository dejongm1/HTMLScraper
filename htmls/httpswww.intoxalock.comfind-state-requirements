 $(document).ready(function() {
 
            // Optimalisation: Store the references outside the event handler:
            var $window = $(window);

            function checkWidth() {
			
					var windowsize = window.innerWidth;
					
					if (windowsize < 768) {
											
								$( "footer.new-footer-wrap h3" ).click(function(evt) {
								
									$(this).next(".open-close-mobile").slideToggle("slow");
									
									$(this).toggleClass("minus-icon");
									
									evt.stopImmediatePropagation();
								  
								});		
								
								$(".social-wrapper").insertBefore(".footer-section.row");
							
					}
					
					else {
			   
								$(".social-wrapper").insertBefore(".copyright-wrapper");
												
					}
				
            }
            // Execute on load
            checkWidth();
            // Bind event listener
            $(window).resize(checkWidth);
        });


</script>
